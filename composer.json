{
    "name": "numesia/mailjet",
    "description": "Mailjet Notifications driver",
    "homepage": "https://github.com/numesia/mailjet",
    "license": "MIT",
    "authors": [
        {
            "name": "Amine OUDJEHIH",
            "email": "amine.oudjehih@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "guzzlehttp/guzzle": "~7.0",
        "illuminate/notifications": "^5.5 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "illuminate/support": "^5.5 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "mailjet/laravel-mailjet": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Numesia\\Mailjet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Numesia\\Mailjet\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
